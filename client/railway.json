{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "DOCKERFILE",
        "dockerfilePath": "client/Dockerfile",
        "buildContext": "client"
    },
    "deploy": {
        "numReplicas": 1,
        "healthcheckPath": "/",
        "restartPolicyType": "ON_FAILURE"
    }
}